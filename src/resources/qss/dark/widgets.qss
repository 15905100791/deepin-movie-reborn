/* -------- Play list ----------------*/

dmr--PlaylistWidget {
    background-color: rgba(23, 23, 23, 0.80);
}

Dtk--Widget--DImageButton#CloseBtn {
    qproperty-normalPic: url(:/resources/icons/dark/normal/close-normal.png);
    qproperty-hoverPic: url(:/resources/icons/dark/normal/close-hover.png);
    qproperty-pressPic: url(:/resources/icons/dark/normal/close-press.png);
}

/* state Hover*/
*[PlayItemThumb="true"][ItemState="2"] {
    qproperty-bg: url(:/resources/icons/dark/normal/film-bg.png);
    background-color: rgba(0, 0, 0, 0.1);
}


*[PlayItemThumb="true"][ItemState="2"] *[Name="true"] {
    font-size: 12px;
    line-height: 1.5;
    color: #ffffff;
}

*[PlayItemThumb="true"][ItemState="2"] *[Time="true"] {
    font-size: 10px;
    line-height: 1.5;
    color: rgba(255, 255, 255, 0.6);
}

/* state Playing */
*[PlayItemThumb="true"][ItemState="1"] {
    qproperty-bg: url(:/resources/icons/dark/normal/film-bg.png);
}


*[PlayItemThumb="true"][ItemState="1"] *[Name="true"] {
  font-size: 12px;
  line-height: 1.5;
  color: #01bdff;
}

*[PlayItemThumb="true"][ItemState="1"] *[Time="true"] {
    font-size: 10px;
    line-height: 1.5;
    color: rgba(1, 189, 255, 0.6);
}

/* state Normal */
*[PlayItemThumb="true"][ItemState="0"] {
    qproperty-bg: url(:/resources/icons/dark/normal/film-bg.png);
}


*[PlayItemThumb="true"][ItemState="0"] *[Name="true"] {
    font-size: 12px;
    line-height: 1.5;
    color: #ffffff;
}

*[PlayItemThumb="true"][ItemState="0"] *[Time="true"] {
    font-size: 10px;
    line-height: 1.5;
    color: rgba(255, 255, 255, 0.6);
}

/* -------- End of Play list ----------------*/

