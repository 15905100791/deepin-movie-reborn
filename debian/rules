#!/usr/bin/make -f
include /usr/share/dpkg/default.mk
export QT_SELECT=5
DEB_HOST_MULTIARCH ?= $(shell dpkg-architecture -qDEB_HOST_MULTIARCH)

%:
	dh $@

override_dh_auto_configure:
	dh_auto_configure -- -DCMAKE_INSTALL_PREFIX=/usr -DUSE_DXCB=off -DCMAKE_BUILD_TYPE=ReLWithDebInfo DEFINES+="VERSION=$(DEB_VERSION_UPSTREAM)"

